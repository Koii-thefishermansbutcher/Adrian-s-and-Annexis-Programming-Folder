name = input("Whats your name: ") 
age = int(input("How old are you: ")) 
address = input("Where are you from: ") 
email = input("What is your email address: ") 
phonenumber = int(input("What is your phone number: ")) 
height = float(input("How tall are you: ")) 
weight = in(input("What is your weight: ")) 

print("Hello! " +name) 
print("You are "+str(age) +" years old") 
print("You are from "+str(address)+" Iloilo City") 
print("Your email "+str(email)+" is verified") 
print("Your phone number "+str(phonenumber) +" is activated") 
print ("You are "+str(height)+" tall") 
print("You're weight is "+str(weight)+" heavy") 
